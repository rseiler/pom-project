<FindBugsFilter>
    <!-- importer -->
    <And>
        <Class name="at.rseiler.kenni.importer.Importer"/>
        <Bug uriPattern="DMI_EMPTY_DB_PASSWORD"/>
    </And>

    <!-- webapp -->
    <Package name="~.*\.generated\..*"/>
    <And>
        <Class name="at.rseiler.kenni.config.WebMvcConfig"/>
        <!--<Bug code="ISC_INSTANTIATE_STATIC_CLASS"/>-->
    </And>
</FindBugsFilter>